{
  "imports": {
    "hono": "jsr:@hono/hono@^4.8.3"
  },
  "tasks": {
    "start": "deno run --allow-sys --allow-net --allow-read --allow-run --allow-env src/main.ts",
    "dev": "DEV=true deno run --allow-sys --allow-net --allow-read --allow-run --allow-env src/main.ts",
    "fmt": "deno run --allow-read --allow-write --allow-env npm:@biomejs/biome@latest format --write ."
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "hono/jsx"
  }
}
